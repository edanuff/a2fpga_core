#! /usr/local/bin/vvp
:ivl_version "13.0 (devel)" "(s20221226-331-g77d7f0b8f)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 12;
:vpi_module "/usr/local/lib/ivl/system.vpi";
:vpi_module "/usr/local/lib/ivl/vhdl_sys.vpi";
:vpi_module "/usr/local/lib/ivl/vhdl_textio.vpi";
:vpi_module "/usr/local/lib/ivl/v2005_math.vpi";
:vpi_module "/usr/local/lib/ivl/va_math.vpi";
:vpi_module "/usr/local/lib/ivl/v2009.vpi";
S_0x141e074c0 .scope package, "$unit" "$unit" 2 1;
 .timescale 0 0;
S_0x141e0f330 .scope module, "debug_clog2" "debug_clog2" 3 2;
 .timescale -9 -12;
P_0x141e07630 .param/l "ACC_WIDTH" 1 3 5, C4<00000000000000000000000000011010>;
P_0x141e07670 .param/l "CLOCK_SPEED_HZ" 1 3 3, C4<00000011001101111111100110000000>;
P_0x141e076b0 .param/l "TICK_RATE" 1 3 4, C4<00000000110110100111101001100101>;
S_0x141e0f520 .scope begin, "$unm_blk_2" "$unm_blk_2" 3 15, 3 15 0, S_0x141e0f330;
 .timescale -9 -12;
v0x141e0f690_0 .var "acc", 26 0;
v0x141e1f500_0 .var/i "cycle_count", 31 0;
v0x141e1f5a0_0 .var/i "tick_count", 31 0;
    .scope S_0x141e0f330;
T_0 ;
    %vpi_call/w 3 8 "$display", "CLOCK_SPEED_HZ = %0d", P_0x141e07670 {0 0 0};
    %vpi_call/w 3 9 "$display", "TICK_RATE = %0d", P_0x141e076b0 {0 0 0};
    %vpi_call/w 3 10 "$display", "ACC_WIDTH = %0d", P_0x141e07630 {0 0 0};
    %vpi_call/w 3 11 "$display", "ACC_WIDTH+1 bits max = %0d", 32'sb00000111111111111111111111111111 {0 0 0};
    %pushi/real 2024770412, 4067; load=3.77143
    %pushi/real 1622695, 4045; load=3.77143
    %add/wr;
    %vpi_call/w 3 12 "$display", "Ratio = %f", W<0,r> {0 1 0};
    %fork t_1, S_0x141e0f520;
    %jmp t_0;
    .scope S_0x141e0f520;
t_1 ;
    %pushi/vec4 0, 0, 27;
    %store/vec4 v0x141e0f690_0, 0, 27;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x141e1f5a0_0, 0, 32;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x141e1f500_0, 0, 32;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x141e1f500_0, 0, 32;
T_0.0 ; Top of for-loop 
    %load/vec4 v0x141e1f500_0;
    %cmpi/s 200, 0, 32;
    %jmp/0xz T_0.1, 5;
    %load/vec4 v0x141e0f690_0;
    %pad/u 32;
    %addi 14318181, 0, 32;
    %pad/u 27;
    %store/vec4 v0x141e0f690_0, 0, 27;
    %load/vec4 v0x141e0f690_0;
    %pad/u 32;
    %cmpi/u 54000000, 0, 32;
    %flag_inv 5; GE is !LT
    %jmp/0xz  T_0.3, 5;
    %load/vec4 v0x141e0f690_0;
    %pad/u 32;
    %subi 54000000, 0, 32;
    %pad/u 27;
    %store/vec4 v0x141e0f690_0, 0, 27;
    %load/vec4 v0x141e1f5a0_0;
    %addi 1, 0, 32;
    %store/vec4 v0x141e1f5a0_0, 0, 32;
T_0.3 ;
T_0.2 ; for-loop step statement
    %load/vec4 v0x141e1f500_0;
    %addi 1, 0, 32;
    %store/vec4 v0x141e1f500_0, 0, 32;
    %jmp T_0.0;
T_0.1 ; for-loop exit label
    %pushi/real 1779401595, 4071; load=53.0303
    %pushi/real 1214670, 4049; load=53.0303
    %add/wr;
    %vpi_call/w 3 30 "$display", "After %0d clock cycles: %0d ticks (expected ~%f)", v0x141e1f500_0, v0x141e1f5a0_0, W<0,r> {0 1 0};
    %end;
    .scope S_0x141e0f330;
t_0 %join;
    %vpi_call/w 3 34 "$finish" {0 0 0};
    %end;
    .thread T_0;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "-";
    "debug_clog2.sv";
